.result-table .text-right{
  border-bottom: 1px solid lightgray;
  border-right: 1px solid lightgray;
  border-left: 1px solid lightgray;
  border-top: 1px solid lightgray;
  padding: 0
}
.table-button {
  box-shadow: none;
  height 80px;
  width: 100%;
  padding: 5px
}
.result-box p{
  margin: 50px
  color: #424242
}
.apexcharts-toolbar {
  display: none !important
}
.class-list {
  text-align: center
}
.class-list .q-list{
  border: none
  margin-top: 50px
}
.class-list-button {
  text-align: center
}
.title {
  text-align: center
  margin-top: 50px
}
.result .q-list {
  border:none
}
.q-table-container {
  box-shadow: none
}
.class-table {
  border: none
}
.chart {
  border: none
}
.result .q-list-header {
  color: #00acc1
}