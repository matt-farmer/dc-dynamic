.result-box .btn-outline-primary{
  display: none
}
.result-box .alert {
  display: none !important
}
.q-layout-header {
  box-shadow: none
}
.q-layout-header.act {
  -webkit-box-shadow: 0 8px 6px -6px gray;
  -moz-box-shadow: 0 8px 6px -6px gray;
  box-shadow: 0 8px 6px -6px gray;
}
.container {
  height: 800px
}
.select-box {
  height: 300px;
  width: 100%
}
.select-box-input {
  text-align: center;
  width: 300px;
  height: 300px;
  margin: 0 auto
}
.input-box {
  box-shadow: none;
  border: 1px solid lightgray
}
.result-box {
  height: 300px;
  text-align: center;
}
.result-box .table {
  margin: 0 auto;
}
.result-box thead {
  color: #003478;
  background-color: #e0f7fa
}
.result-box .font-weight-bold {
  color: #003478;
  background-color: #e0f7fa
}
.result-box tr{
  height: 80px;
}
